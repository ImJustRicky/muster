{
  "project": "myapp",
  "version": "1",
  "root": "/home/deploy/myapp",
  "services": {
    "api": {
      "name": "API Server",
      "type": "docker",
      "port": 8080,
      "health": {
        "type": "http",
        "endpoint": "/health",
        "port": 8080,
        "timeout": 10
      },
      "credentials": {
        "enabled": false,
        "risk_level": "high"
      },
      "depends_on": ["redis", "postgres"]
    },
    "redis": {
      "name": "Redis",
      "type": "docker",
      "port": 6379,
      "health": {
        "type": "tcp",
        "port": 6379,
        "timeout": 5
      }
    },
    "postgres": {
      "name": "PostgreSQL",
      "type": "managed",
      "skip_deploy": true,
      "health": {
        "type": "tcp",
        "port": 5432,
        "timeout": 5
      },
      "credentials": {
        "enabled": false,
        "risk_level": "high"
      }
    }
  },
  "deploy_order": ["redis", "api"],
  "skills": []
}
